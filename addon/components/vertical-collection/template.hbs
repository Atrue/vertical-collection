{{!--
{{#each items key='@index' as |item index|}}
  {{#vertical-item
    tagName=itemTagName
    classNames=itemClassNames
    defaultHeight=_defaultHeight
    alwaysRemeasure=alwaysRemeasure
    content=item
    index=index
    register=(action register)
    unregister=(action unregister)
  }}
    {{yield item index}}
  {{/vertical-item}}
{{else}}
  {{yield to="inverse"}}
{{/each}}
--}}
{{#each activeItems key='id' as |item|}}
  {{#vertical-item
    tagName=itemTagName
    parentToken=token
    item=item
  }}
    {{yield item.content.ref item.content.index}}
  {{/vertical-item}}
{{else}}
  {{yield to="inverse"}}
{{/each}}